{
    "search.exclude": {
        "MODULE.bazel.lock": true,
        ".aspect/bazelrc": true
    },
    "cSpell.words": [
        "dpkg",
        "pydpkg",
        "runfile",
        "runfiles",
        "scanpackages"
    ],
    "files.associations": {
        ".envrc*": "shellscript",
        ".bazelrc": "shellscript"
    },
    "evenBetterToml.formatter.arrayAutoCollapse": false,
    "evenBetterToml.formatter.arrayTrailingComma": true,
    "evenBetterToml.formatter.allowedBlankLines": 1,
}
